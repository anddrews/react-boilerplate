{
  "extends": [
    "airbnb",
    "airbnb/hooks"
  ],
  "rules": {
    "object-curly-spacing": "off",
    "import/prefer-default-export": "off",
    "react/prop-types": 0,
    "max-len": ["error", { "code": 120 }],
    "comma-dangle": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  }
}
